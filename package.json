{
  "name": "vstinder",
  "displayName": "VS Tinder",
  "icon": "media/icon.png",
  "description": "An app that mathches programmers by allowing then to swipe at code.",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaganLohchab/VS_Tinder"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsinder-sidebar-view",
          "title": "VSinder",
          "icon": "./src/media/heart.svg"
        }
      ]
    },
    "views": {
      "vsinder-sidebar-view": [
        {
          "type": "webview",
          "id": "vsinder-sidebar",
          "name": "VSinder",
          "icon": "./src/media/heart.svg",
          "contextualTitle": "VSinder"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.login",
        "title": "Login"
    }
    ]
     
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
